>>> 1==2;1!=2                                            # 数值的比较
False
True
>>> print('a'=='b','a'!='b');print('a'<'b','a'>'b')      # 字母的比较
False True
True False
>>> print(ord('a'),ord('b'))                             # 查看字母编码
97 98
>>> print(chr(97),chr(98))                               # 查看编码对应的字符
a b
>>> '#'<'$'                                              # 字符的比较
True
>>> ord('a');ord('b')                                    # 查看字母编码
97
98
>>> chr(97);chr(98)                                      # 查看编码对应的字符
'a'
'b'